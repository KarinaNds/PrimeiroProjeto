/* CSS reset */

/CSS reset/
*{
    border:0;
    padding:0;
    margin:0;
}

body{
	width:100%;
	height:700px;
}

main{
	position:relative;
    width:100%;
    height:656px;
    background-color:#c5dbf2;
    margin:auto;
}
#cabeçalho{
    position:relative;
    width:100%;
    height:80px;
}
#cabeçalho a{
    width:6.888%;
    font-size:18px;
    background-color:#c5dbf2; 
    color:#000;
    text-decoration: none;
    float:left;
    padding-top:25px;
    padding-left:30px;
    font-family: Arial;
}
#cabeçalho img{
    position:relative;
    float:left;
    margin-left:9%;
    padding-top:15px;
}

#cadastrar a{
    position:relative;
    margin-left:20px;
    width:100%;
    height:20px;
    padding:10px;
    margin-top: 10px;
    font-family: Arial;
    background-color:#007c89;
    color:#fff;
    font-size:16px;
	text-decoration:none;
	float:left;
}
#cadastrar a:hover{
    background-color:#036874;
}

#caixa a{
    position:relative;
    text-align: center;
    border:solid 1px #000;
    width:6%;
    height:20px;
    padding:10px;
    margin-top: 10px;
    font-family: Arial;
	text-decoration:none;
	float:left;
}

#caixa a:hover{
    color:#fff;
}
#cabecalho a:hover{
    color:#fff;
}
#conteudo{
    position:absolute;
    left:200px;
    top:200px;
    width:40%;
    height:300px;
}
#titulo {
    font-family:'Times New Roman', Times, serif;
    font-size: 70px;
    color:#0c0d0d;
}
#conteudo p{
    color:#67696b;
    font-size:27px;
    font-family: Arial, Helvetica, sans-serif;
}
#saiba_mais{
    float: left;
    position:relative;
    text-align: center;
    background-color:#007c89;
    width:30%;
    height:30px;
    padding:10px;
    padding-top:20px;
    margin-top: 10px;
    font-family: Arial;
    color:#fff;
}
#begie{
    position:absolute;
    left:600px;
    top:25px;
    width:80%;
    height:300px;
    background-color: #f7d5c5;
    border-radius: 10px;
    border: solid 2px #000;
}
#img1 img{
    margin-left:10px;
    float: left;
}
#img2 img{
    position: absolute;
    left:405px;
    top:1px;
}
#spa{
    position:relative;
    clear:both;
    height:100px;
    width:100%;
    text-align: center;
    color:#f87a3f;
    font-family: Arial, Helvetica, sans-serif;
    font-size: 30px;

}
#img3 img{
    position: absolute;
    left:30px;
    top:100px;
}
#sobreposto{
    position:absolute;
    left:250px;
    top:130px;
    width:200px;
    height:200px;
    border-radius: 10px;
    background-color: #000;
}
#sobreposto_2{
    position:absolute;
    left:240px;
    top:120px;
    width:200px;
    height:200px;
    border-radius: 10px;
    background-color: #fff;
    border: 1px solid #000;

}
#cart{
    font-family:'Times New Roman', Times, serif;
    padding:10px;
    color: #000;
    background-color: #dbdbdb;
    border-bottom: solid 1px #000;
    border-radius: 10px 10px 0px 0px;
}
#botom{
    position:relative;
    width:90%;
    height: 18px;
    background-color:#007c89;
    margin-left:9px;
    border-radius:2px;
    color:#fff;
    text-align:center;
    font-family:Arial;
    padding:2px;
}

